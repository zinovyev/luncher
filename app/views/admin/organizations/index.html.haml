%p#notice= notice
%h1 Admin Organizations
%table
  %thead
    %tr
      %th{:colspan => "3"}
  %tbody
    - @admin_organizations.each do |admin_organization|
      %tr
        %td= link_to 'Show', admin_organization
        %td= link_to 'Edit', edit_admin_organization_path(admin_organization)
        %td= link_to 'Destroy', admin_organization, method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New Admin Organization', new_admin_organization_path
